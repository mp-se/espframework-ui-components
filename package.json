{
  "name": "@mp-se/espframework-ui-components",
  "version": "1.7.0",
  "type": "module",
  "description": "Reusable Vue components for ESP Framework UI",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mp-se/espframework-ui-components.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,md,css,scss,html}\"",
    "lint": "eslint --ext .js,.vue src demo-app.js demo.html || true",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "rollup": "^4.9.6",
    "rollup-plugin-css-only": "^4.5.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-vue": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "vue",
    "components",
    "esp32",
    "esp-framework",
    "ui",
    "bootstrap"
  ],
  "author": "Magnus Persson",
  "license": "MIT",
  "dependencies": {
    "json-parse-even-better-errors": "^5.0.0"
